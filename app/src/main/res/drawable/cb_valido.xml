<?xml version="1.0" encoding="utf-8"?>
<!-- Se cambia el boton de los checkbox, por default es un cuadrado, este los cambia a estrella -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false"
        android:drawable="@mipmap/ic_selector_off" />
    <item android:state_checked="true"
        android:drawable="@mipmap/ic_selector_on"/>
</selector>